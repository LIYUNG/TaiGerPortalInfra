{
    "name": "tai_ger_portal_infra",
    "version": "0.1.0",
    "bin": {
        "tai_ger_portal_infra": "bin/tai_ger_portal_infra.js"
    },
    "scripts": {
        "clean": "rm -rf build dist cdk.out node_modules",
        "build": "tsc",
        "lint": "eslint --cache lambda lib test constants && prettier --list-different --cache .",
        "lint:fix": "eslint --fix",
        "format": "prettier --write --cache .",
        "prebuild": "rm -rf build dist cdk.out",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "prepare": "husky",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "**/*.js": [
            "prettier --write",
            "eslint --fix --cache lambda lib test constants",
            "git add ."
        ]
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.25.9",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-typescript": "^7.26.0",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.17.0",
        "@types/jest": "^29.5.12",
        "@types/node": "20.14.9",
        "@typescript-eslint/eslint-plugin": "^8.19.0",
        "@typescript-eslint/parser": "^8.19.0",
        "aws-cdk": "^2.149.0",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.3.0",
        "ts-jest": "^29.1.5",
        "ts-node": "^10.9.2",
        "typescript": "~5.5.3",
        "typescript-eslint": "^8.19.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.685.0",
        "@aws-sdk/client-secrets-manager": "^3.682.0",
        "@aws-sdk/client-ses": "^3.721.0",
        "@taiger-common/core": "^1.0.7",
        "@taiger-common/model": "^1.0.23",
        "aws-cdk-lib": "^2.173.4",
        "bottleneck": "^2.19.5",
        "constructs": "^10.4.2",
        "mongodb": "^6.10.0",
        "nodemailer": "^6.9.16",
        "prettier": "^3.3.3",
        "source-map-support": "^0.5.21"
    }
}
